{
    "$schema": "https://turbo.build/schema.json",
    "pipeline": {
        "build": {
            "cache": false,
            "dependsOn": [
                "^build"
            ]
        },
        "start": {
            "dependsOn": [
                "^build"
            ],
            "cache": true
        },
        "test": {
            "dependsOn": [
                "^build"
            ],
            "cache": false
        },
        "dev": {
            "cache": false,
            "persistent": true
        },
        "lint": {}
    }
}